//Find second minimum in an array

#include<stdio.h>
#include<limits.h>
int main()
{
    int n,i,a[50];
    int min1=INT_MAX , min2=INT_MAX;

    printf("Enter size : ");
    scanf("%d",&n);

    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    for(i=0;i<n;i++)
    {
        if(a[i] < min1)
        {
            min2 = min1;
            min1 = a[i];
        }
        else if(a[i] < min2 && a[i] != min1)
        {
            min2 = a[i];
        }
    }

    printf("Second Minimum = %d",min2);
    return 0;
}
