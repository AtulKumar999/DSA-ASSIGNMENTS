//How you will rotate an array using C

#include<stdio.h>

int main()
{
    int a[50],n,i,temp;

    printf("Enter size of array : ");
    scanf("%d",&n);

    printf("Enter %d elements : ",n);
    for(i=0;i<n;i++)
        scanf("%d",&a[i]);

    temp = a[0];    // store first element

    for(i=0;i<n-1;i++)
        a[i] = a[i+1];

    a[n-1] = temp;

    printf("After Rotating Array : ");
    for(i=0;i<n;i++)
        printf("%d ",a[i]);

    return 0;
}
